{
  "version": 2,
  "builds": [
    {
      "src": "my/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "cd my && npm install --legacy-peer-deps && npm run build",
        "outputDirectory": "build",
        "installCommand": "cd my && npm install --legacy-peer-deps"
      }
    },
    {
      "src": "server/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/api/index.js"
    },
    {
      "src": "^/static/(.*)",
      "dest": "my/build/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "^/(.*\.(js|css|png|jpg|jpeg|gif|ico|svg|woff2|woff|ttf|eot))$",
      "dest": "my/build/$1",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "^(.*)$",
      "dest": "my/build/index.html",
      "status": 200
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DISABLE_ESLINT_PLUGIN": "true"
  }
}